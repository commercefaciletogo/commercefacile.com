{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "algolia/algoliasearch-client-php": "^1.17",
        "arcanedev/localization": "~0.8",
        "delight-im/ids": "^1.0",
        "elasticsearch/elasticsearch": "^5.1",
        "geoip2/geoip2": "^2.4",
        "hashids/hashids": "^2.0",
        "hesto/multi-auth": "^1.0",
        "intervention/image": "^2.3",
        "laravel-notification-channels/messagebird": "^1.0",
        "laravel/framework": "5.3.*",
        "laravel/scout": "2.0.2",
        "league/flysystem-dropbox": "^1.0",
        "league/flysystem-rackspace": "1.0",
        "league/fractal": "^0.15.0",
        "mcamara/laravel-localization": "^1.1",
        "pda/pheanstalk": "^3.1",
        "pmatseykanets/laravel-scout-postgres": "^0.5.0",
        "predis/predis": "^1.1",
        "ramsey/uuid": "^3.5",
        "tamayo/laravel-scout-elastic": "2.0.0",
        "torann/geoip": "^1.0",
        "bjrnblm/messagebird": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "themsaid/laravel-langman": "^1.3"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Commerce\\": "app/Commerce"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ],
        "psr-4": {
            "Commerce\\": "app/Commerce"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
